{
  "name": "QuizWhizz",
  "version": "1.0.0",
  "description": "Quiz Whizz exercise with Math Puzzle",
  "main": "gulpfile.js",
  "private": true,
  "scripts": {
    "develop": "concurrently \"tsc -w -p ./server\" \"tsc -w -p ./client\" \"nodemon ./server/bin/www\" ",
    "forever": "tsc -p ./server && forever start ./server/bin/www --watch",
    "twc:client:w": "tsc -w -p ./client",
    "twc:server:w": "tsc -w -p ./server",
    "typings": "typings install",
    "bundle": "node tools/builder.js",
    "bundle:prod": "node tools/builder.js --prod",
    "postinstall": "typings install && tsc -p ./server"
  },
  "engines": {
    "node": ">= 5.4.1 < 6"
  },
  "author": "David Thong Nguyen",
  "license": "MIT",
  "dependencies": {
    "@angular/common": "^2.0.0-rc.5",
    "@angular/compiler": "^2.0.0-rc.5",
    "@angular/core": "^2.0.0-rc.5",
    "@angular/forms": "^0.3.0",
    "@angular/http": "^2.0.0-rc.5",
    "@angular/platform-browser": "^2.0.0-rc.5",
    "@angular/platform-browser-dynamic": "^2.0.0-rc.5",
    "@angular/router": "^3.0.0-rc.1",
    "@angular/upgrade": "^2.0.0-rc.5",
    "angular2-in-memory-web-api": "0.0.15",
    "angular2-jwt": "^0.1.22",
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "core-js": "^2.4.1",
    "debug": "^2.2.0",
    "express": "^4.13.4",
    "express-jwt": "^3.3.0",
    "forever": "^0.15.2",
    "jsonwebtoken": "^5.7.0",
    "ng-semantic": "^1.1.1",
    "reflect-metadata": "^0.1.4",
    "rxjs": "^5.0.0-beta.6",
    "serve-favicon": "~2.3.0",
    "systemjs": "0.19.27",
    "typescript": "next",
    "typings": "^1.3.1",
    "zone.js": "^0.6.21"
  },
  "devDependencies": {
    "concurrently": "^2.2.0",
    "nodemon": "^1.9.1",
    "systemjs-builder": "^0.15.26",
    "yargs": "^4.8.1"
  },
  "peerDependencies": {
    "rxjs": "5.0.0-beta.11",
    "@angular/http": "2.0.0-rc.5",
    "@angular/core": "2.0.0-rc.5"
  }
}
