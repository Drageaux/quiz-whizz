{
  "name": "QuizWhizz",
  "version": "1.0.0",
  "description": "Quiz Whizz exercise with Math Puzzle",
  "main": "gulpfile.js",
  "private": true,
  "scripts": {
    "develop": "concurrently \"tsc -w -p ./server\" \"tsc -w -p ./client\" \"nodemon ./server/bin/www\" ",
    "forever": "tsc -p ./server && forever start ./server/bin/www --watch",
    "twc:client:w": "tsc -w -p ./client",
    "twc:server:w": "tsc -w -p ./server",
    "typings": "typings install",
    "bundle": "node tools/builder.js",
    "bundle:prod": "node tools/builder.js --prod",
    "postinstall": "typings install && tsc -p ./server"
  },
  "engines": {
    "node": ">= 5.4.1 < 6"
  },
  "author": "David Thong Nguyen",
  "license": "MIT",
  "dependencies": {
    "@angular/common": "2.0.0-rc.7",
    "@angular/compiler": "2.0.0-rc.7",
    "@angular/core": "2.0.0-rc.7",
    "@angular/forms": "2.0.0-rc.7",
    "@angular/http": "2.0.0-rc.7",
    "@angular/platform-browser": "2.0.0-rc.7",
    "@angular/platform-browser-dynamic": "2.0.0-rc.7",
    "@angular/router": "3.0.0-rc.3",
    "@angular/upgrade": "2.0.0-rc.7",
    "angular2-in-memory-web-api": "0.0.19",
    "angular2-jwt": "^0.1.22",
    "auth0-lock": "^10.3.0",
    "body-parser": "~1.13.2",
    "concurrently": "^2.2.0",
    "cookie-parser": "~1.3.5",
    "core-js": "^2.4.1",
    "debug": "^2.2.0",
    "dotenv": "^2.0.0",
    "express": "^4.13.4",
    "express-jwt": "^3.3.0",
    "forever": "^0.15.2",
    "jsonwebtoken": "^5.7.0",
    "mongoose": "^4.6.0",
    "mongoose-timestamp": "^0.6.0",
    "ng-semantic": "^1.1.3",
    "nodemon": "^1.9.1",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.12",
    "serve-favicon": "~2.3.0",
    "systemjs": "0.19.27",
    "systemjs-builder": "^0.15.26",
    "typescript": "^2.0.2",
    "typings": "^1.3.2",
    "yargs": "^4.8.1",
    "zone.js": "0.6.21"
  }
}
